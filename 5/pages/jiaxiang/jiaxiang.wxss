page{
  min-height: 100%;
  background-color:rgb(238, 233, 233);
}

.box1{
  width: 70%;
  height: 40px;
  background-color: white;
  border: 3px solid white;
  padding: 20px;
  margin: 20px;
  border-radius: 10px;
  box-shadow:0px 0px 10px #9099a0;
}
.box2{
  width: 80%;
  height: auto;
  background-color: aliceblue;
  border: 3px dashed silver;
  padding: 5px 20px 10px 15px;
  margin: -20px 20px;
  border-radius: 20px;
}
.box3{
  width: 100%;
  height: 40px;
  background-color: white;
  border: 3px solid white;
  margin-top: 10px;
  border-radius: 10px;
  box-shadow:0px 0px 10px #9099a0;
}
.left{
  width: 25%;
  height: 100%;
  float: left; 
}
.top{
  width: 90%;
  height: 80%;
  
}
.bottom{
  width: 90%;
  height: 20%;
  font-size: 15px;
  text-align: center;
  font-family: "仿宋";

}

.zhaopian1{
  width: 85%;
  height: 85%;
  margin: 3px;
  padding: 5px;
  border-radius: 20px;
}

.txt{
  padding: 20rpx 50rpx 20rpx 15rpx;
  align-items: center;
  justify-content: center;
  font-size: 11px;
}
.tex{
  font-weight: bold;
  font-size: 19px;
  padding: 20rpx 50rpx 20rpx 15rpx;
}